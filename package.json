{
    "name": "nest-emp2",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "cross-env NODE_ENV=prod nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=dev nest start",
        "start:dev": "cross-env NODE_ENV=dev nest start --watch",
        "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=prod node dist/server/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "dev": "emp dev -cl false --env dev",
        "emp:prod": "emp build --env prod",
        "emp:test": "emp build --env test",
        "build:ts": "emp build -t --env prod",
        "emp:start": "emp serve",
        "stat": "emp build --analyze",
        "stop": "pm2 delete emp-wechat",
        "prod": "pm2 start pm2.config.js --no-daemon",
        "merge": "yarn stop && yarn prod",
        "dts": "emp dts"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@fastly/performance-observer-polyfill": "^2.0.0",
        "@nestjs/common": "^8.2.3",
        "@nestjs/core": "^8.2.3",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.2.3",
        "@types/js-cookie": "^3.0.2",
        "antd": "^4.19.2",
        "autoprefixer": "^10.4.7",
        "axios": "^0.21.4",
        "cache-manager": "^3.6.0",
        "compression": "^1.7.4",
        "connect-redis": "^6.1.3",
        "cookie-parser": "^1.4.6",
        "ejs": "^3.1.6",
        "express-rate-limit": "^6.3.0",
        "express-session": "^1.17.2",
        "http-proxy-middleware": "^2.0.4",
        "intersection-observer": "^0.12.0",
        "js-base64": "^3.7.2",
        "js-cookie": "^3.0.1",
        "jsonwebtoken": "^8.5.1",
        "mobx": "^6.3.7",
        "mobx-react": "^7.2.1",
        "mobx-react-lite": "^3.2.2",
        "morgan": "^1.10.0",
        "mutationobserver-polyfill": "^1.3.0",
        "node-schedule": "^2.1.0",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "pm2": "^5.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "5.3.0",
        "redis": "^3.1.2",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.2",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "tnwx": "^2.5.6",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@efox/emp": "^2.2.3",
        "@efox/eslint-config-react-prittier-ts": "^1.2.7",
        "@nestjs/cli": "^8.2.3",
        "@nestjs/schematics": "^8.0.8",
        "@nestjs/testing": "^8.2.3",
        "@types/connect-redis": "^0.0.18",
        "@types/express": "^4.17.13",
        "@types/jest": "27.4.1",
        "@types/node": "^16.0.0",
        "@types/node-schedule": "^1.3.2",
        "@types/passport-jwt": "^3.0.6",
        "@types/react": "^17.0.41",
        "@types/react-dom": "^17.0.14",
        "@types/react-router-dom": "^5.3.3",
        "@types/redis": "^2.8.31",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "copy-webpack-plugin": "^10.2.4",
        "cross-env": "^7.0.3",
        "d": "^1.0.1",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "html-inline-code-plugin": "^1.0.5",
        "jest": "^27.2.5",
        "postcss-pxtorem": "^6.0.0",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
