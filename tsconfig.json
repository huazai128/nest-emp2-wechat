{
    "extends": "@efox/emp/emp-tsconfig",
    "compilerOptions": {
      "types": ["@efox/emp/client"],
      "baseUrl": ".",
      "paths": {
          "@src/*": [
              "./src/*"
          ]
      },
      "experimentalDecorators": true,
      "typeRoots": [ "node_modules/@types", "./typings"]
    },
    "include": ["src", "typings"],
    "exclude": ["node_modules", "test", "dist","server", "**/*spec.ts"]
  }